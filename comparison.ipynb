    "import matplotlib.pyplot as plt\n",
    "import time\n",
    "\n",
    "def benchmark(func, *args, n_runs=10):\n",
    "    times = []\n",
    "    for _ in range(n_runs):\n",
    "        start = time.time()\n",
    "        func(*args)\n",
    "        cp.cuda.Stream.null.synchronize()\n",
    "        times.append(time.time() - start)\n",
    "    return np.mean(times) * 1000"
   ]
