    "for size in sizes:\n",
    "    print(f'Размер: {size} точек')\n",
    "    \n",
    "    np_X = np.random.rand(size, 3).astype(np.float32)\n",
    "    np_Y = np.random.rand(size, 3).astype(np.float32)\n",
    "    np_dist_times.append(benchmark(euclidean_distances_np, np_X, np_Y))\n",
    "    \n",
    "    cp_X = cp.random.rand(size, 3, dtype=cp.float32)\n",
    "    cp_Y = cp.random.rand(size, 3, dtype=cp.float32)\n",
    "    cp_dist_times.append(benchmark(euclidean_distances_cp, cp_X, cp_Y))\n",
    "\n",
    "plt.figure(figsize=(10, 6))\n",
    "plt.plot(sizes, np_dist_times, 'o-', label='NumPy', linewidth=2)\n",
