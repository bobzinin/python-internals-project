    "def euclidean_distances_np(X, Y):\n",
    "    return np.sqrt(((X[:, np.newaxis] - Y) ** 2).sum(axis=2))\n",
    "\n",
    "def euclidean_distances_cp(X, Y):\n",
    "    return cp.sqrt(((X[:, cp.newaxis] - Y) ** 2).sum(axis=2))\n",
    "\n",
    "sizes = [100, 200, 500, 1000, 2000]\n",
    "np_dist_times = []\n",
    "cp_dist_times = []\n",
